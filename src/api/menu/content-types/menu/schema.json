    "images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "price": {
      "type": "decimal"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "menu"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    }
  }
}
